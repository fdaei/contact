{
    "name": "yiisoft/yii2-app-advanced",
    "description": "Yii 2 Advanced Project Template",
    "keywords": [
        "yii2",
        "framework",
        "advanced",
        "project template"
    ],
    "homepage": "http://www.yiiframework.com/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii2/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii2"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.0",
        "yiisoft/yii2": "~2.0.45",
        "yiisoft/yii2-bootstrap4": "^2.0",
        "yiisoft/yii2-bootstrap5": "*",
        "yiisoft/yii2-symfonymailer": "~2.0.3",
        "vlucas/phpdotenv": "^5.3",
        "filsh/yii2-oauth2-server": "*",
        "ext-pdo": "*",
        "aminbbb92/yii2-user": "@dev",
        "yii2tech/ar-softdelete": "^1.0",
        "yiisoft/yii2-httpclient": "*",
        "aws/aws-sdk-php": "^3.241",
        "yiisoft/yii2-imagine": "~2.2.0",
        "omnilight/yii2-scheduling": "*",
        "yiisoft/yii2-mongodb": "^2.1",
        "yiisoft/yii2-redis": "~2.0.0",
        "sadi01/yii2-swagger": "dev-master",
        "wbraganca/yii2-dynamicform": "*",
        "notamedia/yii2-sentry": "^1.7",
        "lubosdz/yii2-captcha-extended": "^1.0",
        "kartik-v/yii2-widget-select2": "dev-master",
        "froala/yii2-froala-editor": "3.2.5",
        "kartik-v/yii2-grid": "@dev",
        "kartik-v/yii2-bootstrap4-dropdown": "@dev",
        "kartik-v/yii2-widget-fileinput": "dev-master",
        "guzzlehttp/guzzle": "^7.5",
        "ext-http": "*",
        "bower-asset/clipboard": "dev-master",
        "codeception/module-phpbrowser": "*",
        "voime/yii2-google-maps": "*",
        "sadi01/yii2-more-settings": "*",
        "rosell-dk/webp-convert": "^2.6",
        "2amigos/yii2-selectize-widget": "~1.0"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.1.0",
        "yiisoft/yii2-gii": "~2.2.0",
        "yiisoft/yii2-faker": "~2.0.0",
        "codeception/codeception": "^4.2",
        "codeception/module-asserts": "^1.0",
        "codeception/module-yii2": "^1.0",
        "codeception/module-filesystem": "^2.0",
        "codeception/module-rest": "^2.0",
        "codeception/verify": "~0.5.0 || ~1.1.0",
        "symfony/browser-kit": ">=2.7 <=4.2.4",
        "bizley/migration": "^4.0"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "php-http/discovery": true
        },
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
